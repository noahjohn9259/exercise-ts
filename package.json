{
  "name": "create-react-app-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@atlaskit/css-reset": "^3.0.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.5.0",
    "formik": "^1.0.3",
    "history": "^4.7.2",
    "react": "^16.4.2",
    "react-beautiful-dnd": "^9.0.2",
    "react-dom": "^16.4.2",
    "react-popper": "^1.0.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.13",
    "styled-components": "^3.4.2",
    "yup": "^0.26.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --importer=node_modules/node-sass-tilde-importer",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --importer=node_modules/node-sass-tilde-importer --watch --recursive --usePolling --polling-interval 500",
    "start-js": "react-scripts-ts start",
    "start": "npx cross-env PORT=6001 npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-beautiful-dnd": "^7.1.2",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.0",
    "@types/reactstrap": "^6.4.2",
    "@types/shortid": "^0.0.29",
    "@types/webpack": "^4.4.10",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.24.9",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "node-sass-tilde-importer": "^1.0.2",
    "npm-run-all": "^4.1.3",
    "npx": "^10.2.0",
    "typescript": "^3.0.1"
  }
}
