{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier", "@blueprintjs/tslint-config/blueprint-rules"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "blueprint-classes-constants": false,
    "blueprint-html-components": false,
    "member-access": false,
    "ordered-imports": false,
    "no-empty-interface": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "jsx-boolean-value": false
  }
}